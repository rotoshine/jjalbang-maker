<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>짤방생성기</title>  
  <% if(htmlWebpackPlugin.options.googleAdClientKey) { %>
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <script>
      (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: "<%= htmlWebpackPlugin.options.googleAdClientKey %>",
        enable_page_level_ads: true
      });
  </script>
  <% } %>  
</head>
<body>
<div id="firebaseui-auth-container"></div>
<div id="app"></div>
<script type="application/javascript" src="//ajax.googleapis.com/ajax/libs/webfont/1.4.10/webfont.js"></script>
<script type="application/javascript" src="//www.gstatic.com/firebasejs/3.7.5/firebase.js"></script>
<script type="application/javascript">
  // Initialize Firebase
  const config = {
    apiKey: "<%= htmlWebpackPlugin.options.firebaseConfig.apiKey %>",
    authDomain: "<%= htmlWebpackPlugin.options.firebaseConfig.authDomain %>",
    databaseURL: "<%= htmlWebpackPlugin.options.firebaseConfig.databaseURL %>",
    projectId: "<%= htmlWebpackPlugin.options.firebaseConfig.projectId %>",
    storageBucket: "<%= htmlWebpackPlugin.options.firebaseConfig.storageBucket %>",
    messagingSenderId: "<%= htmlWebpackPlugin.options.firebaseConfig.messagingSenderId %>"
  };
  window.firebase.initializeApp(config);
</script>
<% if(htmlWebpackPlugin.options.ga) { %>
  <script type="application/javascript">
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

    ga('create', '<%= htmlWebpackPlugin.options.ga%>', 'auto');
    ga('send', 'pageview');
</script>
<% } %>
<script>window.twttr = (function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0],
    t = window.twttr || {};
  if (d.getElementById(id)) return t;
  js = d.createElement(s);
  js.id = id;
  js.src = "https://platform.twitter.com/widgets.js";
  fjs.parentNode.insertBefore(js, fjs);

  t._e = [];
  t.ready = function(f) {
    t._e.push(f);
  };

  return t;
}(document, "script", "twitter-wjs"));</script>
<!-- built files will be auto injected -->
</body>
</html>
